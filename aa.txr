                        source_indicator = " [üè¢ Ombordan]" if lang == 'uz' else " [üè¢ –°–æ —Å–∫–ª–∞–¥–∞]"
                    materials_text += f"‚Ä¢ {esc(it['name'])} ‚Äî {qty_txt} (üí∞ {price_txt}){source_indicator}\n"
            else:
